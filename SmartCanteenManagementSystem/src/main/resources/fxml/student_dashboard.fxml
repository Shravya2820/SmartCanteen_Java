<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="20.0" layoutX="200.0" layoutY="100.0">
        <Label text="Welcome to the Student Dashboard" styleClass="header-label"/>
        <Button text="Order Food" onAction="#handleOrderFood" styleClass="dashboard-button"/>
        <Button text="View Tokens" onAction="#handleViewTokens" styleClass="dashboard-button"/>
        <Button text="Logout" onAction="#handleLogout" styleClass="dashboard-button"/>
    </VBox>
</AnchorPane>