<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0" styleClass="background">
    <children>
        <Label layoutX="200.0" layoutY="30.0" text="Student Registration" textAlignment="CENTER">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TextField fx:id="usernameField" layoutX="150.0" layoutY="100.0" promptText="Username" />
        <TextField fx:id="emailField" layoutX="150.0" layoutY="150.0" promptText="Email" />
        <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="200.0" promptText="Password" />
        <Button fx:id="registerButton" layoutX="250.0" layoutY="250.0" text="Register" />
        <Button fx:id="backButton" layoutX="350.0" layoutY="250.0" text="Back to Login" />
    </children>
</AnchorPane>